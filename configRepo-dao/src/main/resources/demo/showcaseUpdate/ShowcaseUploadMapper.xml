<mapper namespace="com.framework.demo.dao.showcaseUpload.ShowcaseUploadMapper">
    <select id="findBySearchable"   parameterType="cn.vansky.framework.core.orm.mybatis.plugin.search.entity.search.Searchable" resultMap="BaseResultMap">
